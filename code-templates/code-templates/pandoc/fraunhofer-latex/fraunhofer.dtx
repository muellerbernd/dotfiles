    \leavevmode%
    \hbox{%
      \ltx@ifpackageloaded{beamerposter}{%
        \vbox to 7\FH@beamer@unitlength{%
          \vskip .5\FH@beamer@unitlength
          \begin{beamercolorbox}[
            wd=66\FH@beamer@unitlength,
