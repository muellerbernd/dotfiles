% basic settings.
%
%    \begin{macrocode}
%<*scrlttr2>
\KOMAoptions{paper=a4,fontsize=10pt,fromalign=locationleft,firsthead,
  fromlogo,backaddress=plain,parskip=full-,pagenumber=footright}
