%
%
% \DescribeMacro{title page}
% The following command implements the title page.
%
%    \begin{macrocode}
\setbeamertemplate{title page}{%
  \ifbeamercolorempty[bg]{title}{}{\nointerlineskip}%
    \begin{beamercolorbox}{title}%
      \usebeamerfont{title}%
      \strut\inserttitle\strut
    \end{beamercolorbox}%
    \ifthenelse{\equal{\insertsubtitle}{\@empty}}{}{%
      \ifbeamercolorempty[bg]{subtitle}{}{\nointerlineskip}%
      \begin{beamercolorbox}{subtitle}%
       \usebeamerfont{subtitle}%
        \strut\insertsubtitle\strut
      \end{beamercolorbox}}%
    \nointerlineskip%
    \begin{beamercolorbox}[
      colsep=.1\FH@beamer@unitlength]{separation line}
    \end{beamercolorbox}%
    \vfill%
    \ltx@ifpackageloaded{beamerposter}{}{
      \inserttitlegraphic%
      \vfill%
    }
    \ifbeamercolorempty[bg]{author}{}{\nointerlineskip}%
    \begin{beamercolorbox}{author}%
      \usebeamerfont{author}%
      \insertauthor
    \end{beamercolorbox}%
    \vskip 1ex
    \ifbeamercolorempty[bg]{institute}{}{\nointerlineskip}%
    \begin{beamercolorbox}{institute}
      \usebeamerfont{institute}%
      \insertinstitute%
    \end{beamercolorbox}
    \ifbeamercolorempty[bg]{date}{}{\nointerlineskip}%
    \begin{beamercolorbox}{date}%
      \usebeamerfont{date}%
      \insertdate
    \end{beamercolorbox}%
    \ltx@ifpackageloaded{beamerposter}{%
      \vfill}{%
      \vskip-\beamer@framebottomskip}}
%    \end{macrocode}
%
% \DescribeMacro{items}
% \DescribeMacro{itemize/enumerate}
