% \DescribeMacro{\url}
% The package tries to automatically load the package \ctanref{url}
% for typesetting \emph{Uniform Resource Locators} (URLs).  If the
% package is not available, a dummy macro \cs{url} is installed.
%
%    \begin{macrocode}
\AtBeginDocument{%
  \ltx@ifundefined{url}{%
    \IfFileExists{url.sty}{%
      \RequirePackage{url}
      \urlstyle{same}}{%
      \providecommand{\url}[1]{\mbox{#1}}}
    \ltx@ifundefined{urlstyle}{%
      \PackageWarningNoLine{fraunhofer}{%
        Using dummy macro \string\url,
        consider installing package `url'}}{}}{%
    \@ifpackageloaded{url}{%
      \PackageInfo{fraunhofer}{%
        Macro \string\url\space provided by package `url'}}{%
      \PackageInfo{fraunhofer}{%
        Dummy macro \string\url\space provided}}}}
