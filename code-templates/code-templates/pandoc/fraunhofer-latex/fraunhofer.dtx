%
% The
% \href{http://home.htp-tel.de/lottermose2/tex/index.html}{|digsig|}
% package uses an internal macro \cs{PDFFormExtension@Signature} with an
% \href{https://www.adobe.com/content/dam/acom/en/devnet/acrobat/pdfs/pdfmarkreference.pdf}{pdfmark}
% construct which is redefined here in order to include a border line
% with zero thickness to make the signature field disappear in
% \href{http://www.acrobat.adobe.com/}{Adobe Acrobat}.
%
%    \begin{macrocode}
\ifthenelse{\isundefined{\digsigfield}}{}{%
  \renewcommand{\PDFFormExtension@Signature}{%
    /T (\Fld@name)
    /Subtype /Widget
    /FT /Sig
    /F \ifFld@hidden 2\else 4\fi\space
    /Q \Fld@align\space
    /BS << /W 0 >>
}}
%    \end{macrocode}
%
% \DescribeMacro{\fhgsigsetup}
% The interface controlling the appearance of all signatures is defined.
%
%    \begin{macrocode}
\def\FH@sig@decoration{line}
\def\FH@sig@bgcolor{mountainmeadow!5}
\def\FH@sig@linecolor{black}
\newlength{\FH@sig@width}
\newlength{\FH@sig@height}
\newlength{\FH@sig@linewidth}
\setlength{\FH@sig@width}{18em}
\setlength{\FH@sig@height}{6ex}
\setlength{\FH@sig@linewidth}{\fboxrule}
\define@key{FH@sig@}{decoration}{\def\FH@sig@decoration{#1}}
\define@key{FH@sig@}{bgcolor}{\def\FH@sig@bgcolor{#1}}
\define@key{FH@sig@}{linecolor}{\def\FH@sig@linecolor{#1}}
\define@key{FH@sig@}{width}{\setlength{\FH@sig@width}{#1}}
\define@key{FH@sig@}{height}{\setlength{\FH@sig@height}{#1}}
\define@key{FH@sig@}{linewidth}{\setlength{\FH@sig@linewidth}{#1}}
\newcommand*{\fhgsigsetup}[1]{\setkeys{FH@sig@}{#1}}
%    \end{macrocode}
%
% \DescribeMacro{\fhgsigfield}
% The macro for typesetting the signature field itself is defined.
%
%    \begin{macrocode}
\newboolean{FH@sig@procura}
\newsavebox{\FH@sig@ppbox}
\newlength{\FH@sig@ppboxwidth}
\newcommand*{\fhgsigfield}[2][false]{{%
  \setlength{\fboxsep}{0pt}%
  \setboolean{FH@sig@procura}{#1}%
  \ifthenelse{\boolean{FH@sig@procura}}{%
    \savebox{\FH@sig@ppbox}{\procuraname}}{%
    \savebox{\FH@sig@ppbox}{}}%
  \settowidth{\FH@sig@ppboxwidth}{\usebox{\FH@sig@ppbox}}%
  \ifthenelse{\equal{\FH@sig@decoration}{plain}}{%
    \setlength{\fboxrule}{0pt}%
    \setlength{\FH@sig@linewidth}{0pt}}{%
    \ifthenelse{\equal{\FH@sig@decoration}{box}}{%
      \setlength{\fboxrule}{\FH@sig@linewidth}%
      \setlength{\FH@sig@linewidth}{0pt}}{%
      \ifthenelse{\equal{\FH@sig@decoration}{line}}{%
        \setlength{\fboxrule}{0pt}}{%
        \PackageError{fraunhofer}{Invalid decoration style}{}}}}%
  \fcolorbox{\FH@sig@linecolor}{\FH@sig@bgcolor}{%
    \begin{minipage}[b]{\FH@sig@width-2\fboxrule}
      \begin{minipage}[c][%
        \dimexpr \FH@sig@height-2\fboxrule-\FH@sig@linewidth]{%
          \FH@sig@ppboxwidth}
            \strut\usebox{\FH@sig@ppbox}
      \end{minipage}%
      \begin{minipage}[c]{\dimexpr \FH@sig@width-\FH@sig@ppboxwidth}
        \ifthenelse{\isundefined{\digsigfield}}{}{%
          \begin{Form}
            \digsigfield{%
              \dimexpr \FH@sig@width-2\fboxrule-\FH@sig@ppboxwidth}{%
              \dimexpr \FH@sig@height-2\fboxrule-\FH@sig@linewidth}{#2}%
          \end{Form}%
        }
      \end{minipage}
      \ifthenelse{\equal{\FH@sig@decoration}{line}}{%
        \newline
        \textcolor{\FH@sig@linecolor}{%
          \rule{\dimexpr \FH@sig@width-2\fboxrule}{\FH@sig@linewidth}}%
      }{}%
    \end{minipage}}}}
%    \end{macrocode}
