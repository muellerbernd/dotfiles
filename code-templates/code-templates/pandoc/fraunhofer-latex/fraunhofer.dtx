%
% \subsubsection*{Color theme}
%
% This section contains the implementation of the color theme file.
%
%    \begin{macrocode}
%<*beamercolor>
\ProvidesPackage{beamercolorthemefraunhofer}
  [2022/07/14 v1.6.0 Fraunhofer beamer color theme]
%    \end{macrocode}
%
% This package requires the \gitref[fraunhofer-latex]{beamerthemefraunhofer}
% main theme being loaded.
%
%    \begin{macrocode}
\@ifpackageloaded{beamerthemefraunhofer}{}{%
  \PackageError{beamercolorthemefraunhofer}{%
    Main theme `fraunhofer' not loaded}{}
}
%    \end{macrocode}
%
% Color theme options are processed and the presentation mode is enabled.
%
%    \begin{macrocode}
\ProcessOptionsBeamer
\mode<presentation>
%    \end{macrocode}
%
% This section processes the color information for most elements.
%
%    \begin{macrocode}
\setbeamercolor{title}{fg=black}
\setbeamercolor{subtitle}{fg=black}
\setbeamercolor{frametitle}{fg=mountainmeadow}
\setbeamercolor{framesubtitle}{fg=gray}
\setbeamercolor{headline}{fg=black}
\setbeamercolor{footline}{fg=black}
\setbeamercolor{title in head/foot}{fg=gray}
\setbeamercolor{author in head/foot}{fg=gray}
\setbeamercolor{date in head/foot}{fg=gray}
\setbeamercolor{page number in head/foot}{fg=gray}
\setbeamercolor{institute in head/foot}{fg=black}
\setbeamercolor{alerted text}{fg=lipstick}
\setbeamercolor{structure}{fg=mountainmeadow}
%    \end{macrocode}
%
% The following commands adjust the color of the separation lines.
%
%    \begin{macrocode}
\setbeamercolor{separation line}{bg=mountainmeadow}
\setbeamercolor{lower separation line head}{bg=mountainmeadow}
\setbeamercolor{upper separation line foot}{bg=mountainmeadow}
\setbeamercolor{navigation symbols}{fg=gray!50}
%    \end{macrocode}
%
%
% \DescribeMacro{itemize}
% The following commands adjust the color of itemize environments.
%
%    \begin{macrocode}
\setbeamercolor*{itemize item}{fg=mountainmeadow}
\setbeamercolor*{itemize subitem}{fg=gray}
\setbeamercolor*{itemize subsubitem}{fg=gray!50}
%    \end{macrocode}
%
%
% \DescribeMacro{block example}
% The following section configures |block example| environments.
%
%    \begin{macrocode}
\setbeamercolor{block title example}{fg=orient,bg=orient!10}
\setbeamercolor{block body example}{fg=orient}
\ltx@ifpackageloaded{beamerposter}{}{
  \setbeamercolor{block body example}{bg=orient!5}}
\addtobeamertemplate{block example begin}{%
  \setbeamercolor{itemize item}{fg=orient}}{}
%    \end{macrocode}
%
%
% \DescribeMacro{block alerted}
% The following section configures |alerted block| environments.
%
%    \begin{macrocode}
\setbeamercolor{block title alerted}{fg=lipstick,bg=lipstick!10}
\setbeamercolor{block body alerted}{fg=lipstick}
\ltx@ifpackageloaded{beamerposter}{}{
  \setbeamercolor{block body alerted}{fg=lipstick,bg=lipstick!5}}
\addtobeamertemplate{block alerted begin}{%
  \setbeamercolor{itemize item}{fg=lipstick}}{}
%    \end{macrocode}
%
%
% \DescribeMacro{block}
% The following section configures ordinary |block| environments.
%
%    \begin{macrocode}
\setbeamercolor{block title}{fg=mountainmeadow,bg=mountainmeadow!10}
\ltx@ifpackageloaded{beamerposter}{
  \setbeamercolor{block body}{fg=black}}{
  \setbeamercolor{block body}{fg=mountainmeadow,bg=mountainmeadow!5}}
\addtobeamertemplate{block begin}{%
  \setbeamercolor{itemize item}{fg=mountainmeadow}}{}
%    \end{macrocode}
%
% Finally, the presentation mode is disabled.
%
%    \begin{macrocode}
\mode<all>
%</beamercolor>
%    \end{macrocode}
