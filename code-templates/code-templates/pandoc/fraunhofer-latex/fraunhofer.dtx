%
% \DescribeMacro{\fhglogo}
% Now, the macro itself is being implemented.
%
%    \begin{macrocode}
\newcommand*{\fhglogo}[2][iconsize=16mm]{%
  \setkeys{FH@logo@}{#1}%
  \savebox{\FH@logo@minibox}{%
    \includegraphics[width=10pt]{fhgicon}\hspace{3pt}%
    \fontsize{10}{12}\selectfont%
    \textbf{Fraunhofer}}%
  \settowidth{\FH@logo@miniwidth}{\usebox{\FH@logo@minibox}}%
  \savebox{\FH@logo@box}{%
    \ifthenelse{\equal{#2}{\@empty}}{%
      \usebox{\FH@logo@minibox}}{%
      \shortstack[l]{%
        \usebox{\FH@logo@minibox} \\
        \begin{minipage}[b]{\FH@logo@miniwidth}%
          \flushright%
          \fontsize{6}{7.2}\selectfont%
          \textbf{#2}
        \end{minipage}}}}%
  \ifthenelse{\value{FH@logo@mode}=0}{%
    \resizebox{\FH@logo@size}{!}{%
      \usebox{\FH@logo@box}}}{}%
  \ifthenelse{\value{FH@logo@mode}=1}{%
    \resizebox{!}{\FH@logo@size}{%
      \usebox{\FH@logo@box}}}{}%
