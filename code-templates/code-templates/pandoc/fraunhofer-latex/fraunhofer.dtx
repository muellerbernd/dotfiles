%    \begin{macrocode}
\setkomavar*{date}{}
\setkomavar{date}{%
  \makebox[\useplength{locwidth}][l]{%
    \Ifkomavarempty{place}{}{%
      \usekomavar{place}\usekomavar{placeseparator}}%
