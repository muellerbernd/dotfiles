%
% \DescribeMacro{title}
% \DescribeMacro{subtitle}
% \DescribeMacro{author}
% \DescribeMacro{institute}
% \DescribeMacro{date}
% The following commands adjust settings referring to the title page.
%
%    \begin{macrocode}
\@ifpackageloaded{beamerposter}{
  \setbeamerfont{title}{series=\bfseries,size=\veryHuge}
  \setbeamerfont{subtitle}{series=\mdseries,size=\LARGE}
  \setbeamerfont{author}{series=\bfseries,size=\Large}
  \setbeamerfont{institute}{size=\Large}
  \setbeamerfont{date}{size=\Large}
}{
  \setbeamerfont{title}{series=\bfseries,size=\LARGE}
  \setbeamerfont{subtitle}{series=\mdseries,size=\large}
  \setbeamerfont{author}{series=\bfseries,size=\normalsize}
  \setbeamerfont{institute}{size=\normalsize}
  \setbeamerfont{date}{size=\normalsize}
}
%    \end{macrocode}
%
%
% \DescribeMacro{frametitle}
% \DescribeMacro{framesubtitle}
% The following commands adjust settings referring to the frametitle.
%
%    \begin{macrocode}
\setbeamerfont{frametitle}{series=\bfseries,size=\LARGE}
\setbeamerfont{framesubtitle}{series=\bfseries,size=\large}
%    \end{macrocode}
%
%
% \DescribeMacro{head/foot}
% The following commands adjust settings referring to the head- and footline.
%
%    \begin{macrocode}
\@ifpackageloaded{beamerposter}{
  \setbeamerfont{title in head/foot}{size=\normalsize}
  \setbeamerfont{author in head/foot}{size=\normalsize}
  \setbeamerfont{date in head/foot}{size=\normalsize}
  \setbeamerfont{page number in head/foot}{size=\normalsize}
}{
  \setbeamerfont{title in head/foot}{series=\scriptsize}
  \setbeamerfont{author in head/foot}{size=\scriptsize}
  \setbeamerfont{date in head/foot}{size=\scriptsize}
  \setbeamerfont{page number in head/foot}{size=\scriptsize}
}
%    \end{macrocode}
%
% Miscellaneous settings for the \ctanref{beamerposter} package.
%
%    \begin{macrocode}
\ltx@ifpackageloaded{beamerposter}{
  \setbeamerfont{block title}{series=\bfseries}
  \setbeamerfont{structure}{series=\bfseries}
  \setbeamerfont{alerted text}{series=\bfseries}
}{}
%    \end{macrocode}
%
% Finally, the presentation mode is disabled.
%
%    \begin{macrocode}
\mode<all>
%</beamerfont>
%    \end{macrocode}
