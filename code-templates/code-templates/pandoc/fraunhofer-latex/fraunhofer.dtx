% \subsubsection*{Page layout}
% \marginnote{\onlyarticle\space\onlyreport\space\onlybook}[-4.7ex]
%
% This section implements the page layout for the classes
% \ctanref{scrartcl}, \ctanref{scrreprt}, and \ctanref{scrbook}.
%
%    \begin{macrocode}
%<*scrartcl|scrreprt|scrbook>
\KOMAoptions{paper=portrait,usegeometry,numbers=noenddot,parskip=full-,
  toc=flat,listof=flat}
%    \end{macrocode}
%
% The KOMA-Script classes usually calculate the page layout using
% the \ctanref{typearea} package.  The \gitref[fraunhofer-latex]{fraunhofer}
% package uses the \ctanref{geometry} package instead.
%
%    \begin{macrocode}
\RequirePackage{geometry}
%    \end{macrocode}
%
% \DescribeMacro{FH@scr@a4paper}
% \DescribeMacro{FH@scr@a5paper}
% The boolean variables |FH@scr@a4paper| and |FH@scr@a5paper| are declared
% for the identification of the paper format.
%
%    \begin{macrocode}
\newboolean{FH@scr@a4paper}
\newboolean{FH@scr@a5paper}
%    \end{macrocode}
%
% The page dimensions are checked against the tolerances given by
% \href{https://en.wikipedia.org/wiki/ISO_216}{EN~ISO~216}.
%
%    \begin{macrocode}
\ifthenelse{
  \lengthtest{\paperheight>295mm}\AND
  \lengthtest{\paperheight<299mm}\AND
  \lengthtest{\paperwidth>208mm}\AND
  \lengthtest{\paperwidth<212mm}}{%
    \setboolean{FH@scr@a4paper}{true}}{%
    \setboolean{FH@scr@a4paper}{false}}
\ifthenelse{
  \lengthtest{\paperheight>208mm}\AND
  \lengthtest{\paperheight<212mm}\AND
  \lengthtest{\paperwidth>147.5mm}\AND
  \lengthtest{\paperwidth<149.5mm}}{%
    \setboolean{FH@scr@a5paper}{true}}{%
    \setboolean{FH@scr@a5paper}{false}}
%    \end{macrocode}
%
% The following command sets the font size and page margins.
%
%    \begin{macrocode}
\ifthenelse{\boolean{FH@scr@a4paper}}{
  \geometry{paper=a4paper,margin=20mm,bindingoffset=5mm}
  \KOMAoption{fontsize}{11pt}}{}
\ifthenelse{\boolean{FH@scr@a5paper}}{
  \geometry{paper=a5paper,margin=16mm,bindingoffset=4mm}
  \KOMAoption{fontsize}{9pt}}{}
%    \end{macrocode}
%
% \DescribeMacro{titlepage}
% The current geometry layout needs to be stored for typesetting the
% title pages which may be more than one.
%
%    \begin{macrocode}
\savegeometry{titlepage}
%    \end{macrocode}
%
% The page layout is finally adjusted by adding the vertical dimensions.
%
%    \begin{macrocode}
\ifthenelse{\boolean{FH@scr@a4paper}}{
  \geometry{top=50mm,bottom=35mm,headheight=2cm,headsep=15mm,foot=20mm}
}{}
\ifthenelse{\boolean{FH@scr@a5paper}}{
  \geometry{top=35mm,bottom=25mm,headheight=2cm,headsep=10mm,foot=14mm}
}{}
%    \end{macrocode}
%
%
%
% \DescribeMacro{ignoremargin}
% This layout is stored in the variable |ignoremargin|.
%
%    \begin{macrocode}
\savegeometry{ignoremargin}
%    \end{macrocode}
%
% \DescribeMacro{\FH@scr@margintotal}
% The total width of the margin column and the gap between the margin column
% and the text body is stored in \cs{FH@scr@margintotal} for later use.
%
%    \begin{macrocode}
\newlength{\FH@scr@margintotal}
\setlength{\FH@scr@margintotal}{\marginparwidth+\marginparsep}
%    \end{macrocode}
%
%
%
% \subsubsection*{Headers and Footers}
% \marginnote{\onlyarticle\space\onlyreport\space\onlybook}[-4.7ex]
%
% The KOMA-Script package \ctanref{scrlayer-scrpage} is included
% for typesetting page styles including headers and footers.
%
%    \begin{macrocode}
\RequirePackage{scrlayer-scrpage}
\KOMAoption{markcase}{nouppercase}
\pagestyle{scrheadings}
%    \end{macrocode}
%
% \DescribeMacro{\ihead}
% \DescribeMacro{\chead}
% \DescribeMacro{\ohead}
% Only the outer position of the header shall be non-empty.
%
%    \begin{macrocode}
\ihead{}
\chead{}
\ohead{\leftmark}
%    \end{macrocode}
%
% \DescribeMacro{\ifoot}
% The inner position of the footer contains the value of |docname|.
%
%    \begin{macrocode}
\ifoot*{%
  \begin{minipage}[t][2ex]{.3333\textwidth+.3333\FH@scr@margintotal}
    \ifthenelse{\NOT\boolean{@twoside}\OR\isodd{\value{page}}}{}{%
      \raggedleft}%
    \usekomavar{docname}%
  \end{minipage}}
%    \end{macrocode}
%
% \DescribeMacro{\cfoot}
% The center position of the footer contains the value of
% |classification|.
%
%    \begin{macrocode}
\cfoot*{%
  \begin{minipage}[t][2ex]{.3333\textwidth+.3333\FH@scr@margintotal}
    \centering\bfseries%
    \textcolor{lipstick}{\usekomavar[\MakeUppercase]{classification}}%
  \end{minipage}}
%    \end{macrocode}
%
% \DescribeMacro{\footmark}
% The macro \cs{footmark} for adjusting the footmark besides the
% pagina is added here.
%
%    \begin{macrocode}
\def\FH@scr@footmark{%
  Fraunhofer\xspace\usekomavar{acronym}\par%
  \@subject\xspace\usekomavar{docnumber}}
\newcommand*{\footmark}[1]{\def\FH@scr@footmark{#1}}
%    \end{macrocode}
%
% \DescribeMacro{\FH@scr@makeFooter}
% The following macro typesets the Fraunhofer |acronym| and the
% value of the variable \cs{@subject} with the corresponding |docnumber|.
%
%    \begin{macrocode}
\newcommand*{\FH@scr@makeFooter}{%
  \begin{minipage}[t][2ex][t]{.2777\textwidth+.2777\FH@scr@margintotal}
    \ifthenelse{\NOT\boolean{@twoside}\OR\isodd{\value{page}}}{%
      \raggedleft}{}%
    \FH@scr@footmark\relax
  \end{minipage}}
%    \end{macrocode}
%
% \DescribeMacro{\FH@scr@makePagina}
% The following macro will typeset the current page number.
%
%    \begin{macrocode}
\newcommand*{\FH@scr@makePagina}{%
  \begin{minipage}[t][2ex][t]{.0555\textwidth+.0555\FH@scr@margintotal}
    \ifthenelse{\NOT\boolean{@twoside}\OR\isodd{\value{page}}}{%
      \raggedleft}{}%
    \thepage%
  \end{minipage}}
%    \end{macrocode}
%
% \DescribeMacro{\ofoot}
% According to the page number, the outer position of the footer is assembled.
%
%    \begin{macrocode}
\ofoot*{%
  \ifthenelse{\NOT\boolean{@twoside}\OR\isodd{\value{page}}}{%
    \FH@scr@makeFooter\FH@scr@makePagina}{%
    \FH@scr@makePagina\FH@scr@makeFooter}}
%    \end{macrocode}
%
% The following macro adjusts the font size of the header and the footer.
%
%    \begin{macrocode}
\setkomafont{pageheadfoot}{\footnotesize}
%</scrartcl|scrreprt|scrbook>
%    \end{macrocode}
%
%
%
% \subsubsection*{Margin column}
% \marginnote{\onlyreport\space\onlybook}[-4.7ex]
%
% The implemantation of the margin column applies to \ctanref{scrreprt}
% and \ctanref{scrbook} only.
%
%    \begin{macrocode}
%<*scrreprt|scrbook>
\geometry{includemp,reversemp}
\ifthenelse{\boolean{FH@scr@a4paper}}{%
  \geometry{marginparwidth=25mm,marginparsep=5mm}}{}
\ifthenelse{\boolean{FH@scr@a5paper}}{%
  \geometry{marginparwidth=18mm,marginparsep=4mm}}{}
\ifthenelse{\boolean{@twoside}}{\geometry{asymmetric}}{}
%    \end{macrocode}
%
% The running head and the page style of the first page of a chapter
% needs to be adjusted.
%
%    \begin{macrocode}
\automark[chapter]{chapter}
\automark*[section]{}
\RedeclareSectionCommand[pagestyle=plain.scrheadings]{chapter}
%    \end{macrocode}
%
% \DescribeMacro{\chapterlinesformat}
% A negative indention of chapter headings to the left margin is required.
%
%    \begin{macrocode}
\renewcommand*{\chapterlinesformat}[3]{%
  \hspace*{-\marginparwidth-\marginparsep}%
  \parbox{\dimexpr\linewidth+\marginparwidth+\marginparsep}{%
    \raggedchapter\@hangfrom{#2}{#3}}}
%    \end{macrocode}
%
% The positions of the headers and the footers have to be patched to
% correctly capture the asymmetric page layout.
%
%    \begin{macrocode}
\AtBeginDocument{
  \KOMAoptions{
    headwidth=textwithmarginpar:-\marginparwidth-\marginparsep
                               : \marginparwidth+\marginparsep,
    footwidth=textwithmarginpar:-\marginparwidth-\marginparsep
                               : \marginparwidth+\marginparsep,
}}
%    \end{macrocode}
%
% \DescribeMacro{\sectionlinesformat}
% The following command moves headers of sections, subsections and
% subsubsections to left margin of the margin column.
%
%    \begin{macrocode}
\renewcommand*{\sectionlinesformat}[4]{%
  \Ifstr{#1}{section}{%
