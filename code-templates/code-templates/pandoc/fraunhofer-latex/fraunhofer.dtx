%    \begin{macrocode}
\newcommand{\FH@settomax}[3]{%
  #2=0pt
  \@for\next:=#3\do{%
    \sbox\z@{\next}%
    \ifdim#1\z@>#2%
      #2=#1\z@
    \fi}}
