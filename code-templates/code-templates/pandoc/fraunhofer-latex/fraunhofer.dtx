            dp=.5\FH@beamer@unitlength]{date in head/foot}%
              \usebeamerfont{date in head/foot}%
              \insertshortdate
          \end{beamercolorbox}%
          \vfil%
        }%
      }{%
        \vbox to 7\FH@beamer@unitlength{%
          \vfil%
          \begin{beamercolorbox}[
            wd=18\FH@beamer@unitlength,
            ht=1.5\FH@beamer@unitlength,
            dp=.5\FH@beamer@unitlength]{page number in head/foot}%
              \usebeamerfont{page number in head/foot}%
              \insertframenumber
          \end{beamercolorbox}
          \ifbeamercolorempty[bg]{date in head/foot}{}{%
            \nointerlineskip}%
          \begin{beamercolorbox}[
            wd=18\FH@beamer@unitlength,
            ht=1.5\FH@beamer@unitlength,
            dp=.5\FH@beamer@unitlength]{date in head/foot}%
              \usebeamerfont{date in head/foot}%
              \insertshortdate
          \end{beamercolorbox}%
          \vfil%
        }%
        \vbox to 7\FH@beamer@unitlength{%
          \vfil%
          \begin{beamercolorbox}[
            wd=48\FH@beamer@unitlength,
            ht=1.5\FH@beamer@unitlength,
            dp=.5\FH@beamer@unitlength]{title in head/foot}%
              \usebeamerfont{title in head/foot}%
              \insertshorttitle
          \end{beamercolorbox}
          \ifbeamercolorempty[bg]{author in head/foot}{}{%
            \nointerlineskip}%
          \begin{beamercolorbox}[
            wd=48\FH@beamer@unitlength,
            ht=1.5\FH@beamer@unitlength,
            dp=.5\FH@beamer@unitlength]{author in head/foot}%
              \usebeamerfont{author in head/foot}%
              \insertshortauthor
          \end{beamercolorbox}%
          \vfil%
        }%
      }%
      \vbox to 7\FH@beamer@unitlength{%
        \vfil%
        \begin{beamercolorbox}[
