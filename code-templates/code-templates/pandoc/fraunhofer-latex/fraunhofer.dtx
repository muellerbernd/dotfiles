  \PackageError{beamerinnerthemefraunhofer}{%
    Main theme `fraunhofer' not loaded}{}
}
%    \end{macrocode}
%
% Inner theme options are processed and the presentation mode is enabled.
%
%    \begin{macrocode}
\ProcessOptionsBeamer
\mode<presentation>
%    \end{macrocode}
%
% This code fragment redefines the key \texttt{t} in the file
% file \texttt{beamerbaseframe.sty} in order to perfectly match the
% position of the title on the titlepage with the top of the frametitles
% of each frame.
%
%    \begin{macrocode}
\define@key{beamerframe}{t}[true]{%
  \beamer@frametopskip=3pt%
  \beamer@framebottomskip=0pt plus 1fill\relax%
  \beamer@frametopskipautobreak=\beamer@frametopskip\relax%
  \beamer@framebottomskipautobreak=\beamer@framebottomskip\relax%
  \def\beamer@initfirstlineunskip{}%
}
%    \end{macrocode}
%
% The contents of each frame shall be top-aligned.
%
%    \begin{macrocode}
\beamer@centeredfalse
%    \end{macrocode}
