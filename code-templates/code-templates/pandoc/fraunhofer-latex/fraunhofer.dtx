\setkomavar{fromlogo}{\fhglogo[iconsize=1cm]{\usekomavar{acronym}}}
\setkomavar{firsthead}{%
  \hfill\Ifkomavarempty{fromlogo}{}{\usekomavar{fromlogo}}%
}
%    \end{macrocode}
%
%
% \DescribeMacro{location}
% The following command defines the sender information for which the KOMA-Script
% variable |location| is defined.
%
%    \begin{macrocode}
\setkomavar{location}{%
  \linespread{1.2}%
  \scriptsize%
  \textbf{\usekomavar{institute}}%
  \Ifkomavarempty{alias}{}{,\\ \textbf{\usekomavar{alias}}}%
  \Ifkomavarempty{acronym}{}{, \textbf{\usekomavar{acronym}}} \par
  \textbf{\usekomavar{director}} \\
  \textbf{\usekomavar*{director}} \par
  \usekomavar{address} \\
  \usekomavar{fromzipcode}\space\usekomavar{place} \par
  \textbf{\usekomavar{fromname}} \\
  \Ifkomavarempty{fromfunction}{}{\usekomavar{fromfunction} \\ }
  \Ifkomavarempty{fromunit}{}{\usekomavar{fromunit} \\ }
  \usekomavar*{fromphone}\space\usekomavar{fromphone}
  \Ifkomavarempty{fromfax}{}{%
    \textbar\space\usekomavar*{fromfax}\space\usekomavar{fromfax}} \\
  \usekomavar{fromemail} \\
  \usekomavar{fromurl}
}
%    \end{macrocode}
%
