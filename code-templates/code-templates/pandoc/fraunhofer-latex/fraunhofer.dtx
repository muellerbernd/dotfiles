\setbeamertemplate{sections/subsections in toc}[square]
\setbeamertemplate{itemize/enumerate body begin}{\normalsize}
\setbeamertemplate{itemize/enumerate subbody begin}{\normalsize}
\setbeamertemplate{itemize/enumerate subsubbody begin}{\normalsize}
%    \end{macrocode}
%
% (enter text here)
%
%    \begin{macrocode}
\ltx@ifpackageloaded{beamerposter}{
  \addtobeamertemplate{block alerted end}{}{\vskip 2\bigskipamount}
  \addtobeamertemplate{block example end}{}{\vskip 2\bigskipamount}
  \addtobeamertemplate{block end}{}{\vskip 2\bigskipamount}
}{}
%    \end{macrocode}
